<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="empbean" class="sd.emp">  
<property name="name" value="Prachi Gehani"></property>  
<property name="add" ref="addbean"></property>  
</bean>  
 
 <bean id="addbean" class="sd.Address">  
<property name="city" value="Mumbai"></property>  
<property name="state" value="Maharastra"></property>  
<property name="contry" value="India"></property>  
</bean> 
  
</beans>  